{
  "name": "vanguard",
  "version": "1.0.0",
  "scripts": {
    "start:student": "ng serve student-portal",
    "start:teacher": "ng serve teacher-portal",
    "build:student": "ng build student-portal",
    "build:teacher": "ng build teacher-portal",
    "build:all": "ng build --all",
    "test:student": "jest --config apps/student-portal/jest.config.js",
    "test:teacher": "jest --config apps/teacher-portal/jest.config.js",
    "test:all": "npm run test:student && npm run test:teacher",
    "lint:student": "ng lint student-portal",
    "lint:teacher": "ng lint teacher-portal",
    "lint:all": "ng lint",
    "format": "prettier --write .",
    "clean": "rimraf dist coverage .angular"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "19.2.7",
    "@angular/compiler": "19.2.7",
    "@angular/core": "19.2.7",
    "@angular/forms": "19.2.7",
    "@angular/platform-browser": "19.2.7",
    "@angular/platform-browser-dynamic": "19.2.7",
    "@angular/router": "19.2.7",
    "@primeng/themes": "^19.1.0",
    "primeicons": "^7.0.0",
    "primeng": "^19.1.0",
    "rxjs": "^7.8.2",
    "tailwindcss-primeui": "^0.6.1",
    "tslib": "^2.8.1",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "19.2.8",
    "@angular/cli": "19.2.10",
    "@angular/compiler-cli": "19.2.7",
    "@types/jasmine": "^5.1.7",
    "@types/jest": "^29.5.14",
    "autoprefixer": "^10.4.21",
    "jasmine-core": "~5.6.0",
    "jest": "^29.7.0",
    "jest-preset-angular": "^14.5.5",
    "karma": "^6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^19.2.2",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.3.2",
    "typescript": "^5.8.3"
  }
}